package joao.alarcon.tests.kabum.analytics.model;

import java.util.List;

public class Sales 
{
	private String name;
	private List<SalesPerYear> merchandiseLines;
	
	
	public Sales() {}
	
	public Sales(String name, List<SalesPerYear> merchandiseLines)
	{
		this.merchandiseLines = merchandiseLines;
		this.name = name;
	}
	
	
	public List<SalesPerYear> getMerchandiseLines() 
	{
		return merchandiseLines;
	}
	public void setMerchandiseLines(List<SalesPerYear> merchandiseLines) 
	{
		this.merchandiseLines = merchandiseLines;
	}

	public String getName() 
	{
		return name;
	}
	public void setName(String name) 
	{
		this.name = name;
	}
}
